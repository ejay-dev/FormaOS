{
  "name": "formaos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check-root": "node scripts/check-root.js",
    "check-env": "node scripts/check-env.js",
    "design:check": "node scripts/design-governance-check.mjs",
    "check:app-links": "node scripts/app-link-integrity-audit.mjs",
    "check:admin-nav": "node scripts/check-admin-nav-integrity.mjs",
    "predev": "npm run check-root && npm run check-env",
    "dev": "next dev",
    "prebuild": "npm run check-root && npm run check-env",
    "build": "__NEXT_PRIVATE_STANDALONE_CONFIG= next build --webpack",
    "prestart": "npm run check-root && npm run check-env",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "stylelint": "stylelint '**/*.{css,scss,sass}' --fix",
    "type-check": "tsc -p tsconfig.typecheck.json",
    "typecheck": "npm run type-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='**/*.{js,jsx,ts,tsx}' --collectCoverageFrom='!**/*.d.ts' --collectCoverageFrom='!**/node_modules/**'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "backstop test",
    "test:visual:approve": "backstop approve",
    "test:visual:reference": "backstop reference",
    "test:lighthouse": "lighthouse-ci autorun",
    "test:a11y": "pa11y-ci --sitemap http://localhost:3000/sitemap.xml",
    "test:db": "node scripts/test-db-integrity.js",
    "test:supabase-health": "node scripts/test-supabase-health.js",
    "test:all": "npm run lint && npm run stylelint && npm run type-check && npm run test:coverage && npm run test:e2e && npm run test:visual && npm run test:lighthouse && npm run test:a11y && npm run test:db",
    "setup:monitoring": "node scripts/monitoring-setup.js",
    "setup:tests": "playwright install && npm run test:visual:reference",
    "qa:smoke": "PLAYWRIGHT_REUSE_SERVER=true playwright test e2e/smoke.spec.ts e2e/app-link-integrity.spec.ts e2e/admin-founder-smoke.spec.ts --reporter=list --workers=1",
    "qa:a11y": "PLAYWRIGHT_REUSE_SERVER=true playwright test e2e/a11y-smoke.spec.ts --reporter=list --workers=1",
    "qa:enterprise": "playwright test e2e/enterprise-invariants.spec.ts e2e/security-invariants.spec.ts e2e/qa-enterprise-smoke.spec.ts --reporter=list",
    "qa:full": "npm run test:all && npm run test:supabase-health",
    "ab-test": "ts-node tests/ab-testing/ab-test-cli.ts",
    "ab-test:list": "npm run ab-test list",
    "ab-test:validate": "npm run ab-test validate",
    "test:ab-testing": "npm run ab-test list && npm run ab-test validate",
    "test:qa:quick": "node tests/quick-qa.js",
    "test:qa:full": "bash tests/run-qa-suite.sh",
    "test:compliance:gdpr": "node tests/compliance/gdpr-compliance.js",
    "test:compliance:soc2": "node tests/compliance/soc2-compliance.js",
    "test:compliance:all": "npm run test:compliance:gdpr && npm run test:compliance:soc2",
    "email:preview:auth": "node scripts/preview-auth-emails.mjs"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@hello-pangea/dnd": "^18.0.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-email/components": "^1.0.4",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@sentry/nextjs": "^10.34.0",
    "@sentry/react": "^10.34.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-table": "^8.21.3",
    "@tiptap/extension-link": "^3.15.1",
    "@tiptap/extension-placeholder": "^3.15.1",
    "@tiptap/react": "^3.15.1",
    "@tiptap/starter-kit": "^3.15.1",
    "@types/archiver": "^7.0.0",
    "@node-saml/node-saml": "^5.1.0",
    "@node-saml/passport-saml": "^5.1.0",
    "@upstash/redis": "^1.36.1",
    "archiver": "^7.0.1",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.24.12",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "leva": "^0.10.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "openai": "^6.21.0",
    "pdf-lib": "^1.17.1",
    "passport": "^0.7.0",
    "posthog-js": "^1.321.2",
    "puppeteer": "^24.35.0",
    "qrcode": "^1.5.4",
    "react": "19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.3",
    "react-email": "^5.2.1",
    "react-hook-form": "^7.70.0",
    "recharts": "^3.6.0",
    "resend": "^6.6.0",
    "speakeasy": "^2.0.0",
    "stripe": "^15.11.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "yaml": "^2.8.2",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^20.19.29",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/speakeasy": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "artillery": "^2.0.27",
    "autoprefixer": "^10.4.23",
    "axe-core": "^4.11.1",
    "backstopjs": "^6.3.25",
    "eslint": "^9",
    "eslint-config-next": "^16.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-testing-library": "^7.15.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "k6": "^0.0.0",
    "lighthouse-ci": "^1.13.1",
    "pa11y": "^9.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.3"
  }
}
