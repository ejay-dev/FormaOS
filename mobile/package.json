{
  "name": "formaos-mobile",
  "version": "1.0.0",
  "description": "FormaOS Mobile - Operational Compliance Operating System",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:web && npx cap copy",
    "build:web": "cd .. && npm run build",
    "dev": "npx cap run ios --livereload --external",
    "dev:android": "npx cap run android --livereload --external",
    "open:ios": "npx cap open ios",
    "open:android": "npx cap open android",
    "sync": "npx cap sync",
    "add:ios": "npx cap add ios",
    "add:android": "npx cap add android",
    "copy": "npx cap copy"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0"
  },
  "author": "FormaOS",
  "license": "MIT"
}